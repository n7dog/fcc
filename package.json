{
	"name": "fcc",
	"version": "3.6.1",
	"private": true,
	"type": "module",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"test": "npx playwright test tests",
		"test:ui": "playwright test --ui",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"fresh": "rm -rf node_modules .svelte-kit playwright-report test-results pnpm-lock.yaml && pnpm install && pnpm format",
		"fresh:start": "pnpm fresh && pnpm dev",
		"unlighthouse": "npx unlighthouse --site fcccanada.ca",
		"unlighthouse:dev": "npx unlighthouse --site dev.fcccanada.ca"
	},
	"devDependencies": {
		"@dimforge/rapier3d-compat": "^0.11.2",
		"@floating-ui/dom": "^1.2.8",
		"@iconify/svelte": "^3.1.3",
		"@js-temporal/polyfill": "^0.4.4",
		"@playwright/test": "^1.34.3",
		"@sanity/client": "^6.1.2",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^8.1.0",
		"@semantic-release/npm": "^10.0.3",
		"@skeletonlabs/skeleton": "^1.6.2",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-vercel": "^3.0.0",
		"@sveltejs/kit": "^1.20.0",
		"@tailwindcss/forms": "^0.5.3",
		"@theatre/core": "^0.6.1",
		"@theatre/studio": "^0.6.1",
		"@threlte/core": "^5.0.9",
		"@threlte/extras": "^4.9.3",
		"@threlte/preprocess": "^0.0.2",
		"@threlte/rapier": "^0.5.0",
		"@threlte/theatre": "^0.0.2",
		"@types/three": "^0.152.1",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.41.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.24",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"semantic-release": "^21.0.2",
		"svelte": "^3.59.1",
		"svelte-check": "^3.4.3",
		"svelte-icons": "^2.1.0",
		"svelte-preprocess": "^5.0.4",
		"svelte-sequential-preprocessor": "^1.0.0",
		"svelte-youtube-embed": "^0.1.20",
		"tailwindcss": "^3.3.2",
		"three": "^0.152.2",
		"typescript": "^5.0.4",
		"vite": "^4.3.9",
		"vitest": "^0.31.1"
	}
}
