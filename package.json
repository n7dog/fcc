{
	"name": "fcc",
	"version": "3.9.1",
	"private": true,
	"type": "module",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"test": "npx playwright test tests",
		"test:ui": "playwright test --ui",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"fresh": "rm -rf node_modules .svelte-kit playwright-report test-results pnpm-lock.yaml && pnpm install && pnpm format",
		"fresh:start": "pnpm fresh && pnpm dev",
		"unlighthouse": "npx unlighthouse --site fcccanada.ca",
		"unlighthouse:dev": "npx unlighthouse --site dev.fcccanada.ca"
	},
	"devDependencies": {
		"@floating-ui/dom": "^1.4.5",
		"@iconify/svelte": "^3.1.4",
		"@js-temporal/polyfill": "^0.4.4",
		"@playwright/test": "^1.36.1",
		"@sanity/client": "^6.1.7",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^9.0.3",
		"@semantic-release/npm": "^10.0.4",
		"@skeletonlabs/skeleton": "^1.9.0",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-vercel": "^3.0.2",
		"@sveltejs/kit": "^1.22.3",
		"@tailwindcss/forms": "^0.5.4",
		"@types/node": "^20.4.2",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.45.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.26",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"semantic-release": "^21.0.7",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.6",
		"svelte-icons": "^2.1.0",
		"svelte-preprocess": "^5.0.4",
		"svelte-sequential-preprocessor": "^2.0.1",
		"svelte-youtube-embed": "^0.2.1",
		"tailwindcss": "^3.3.3",
		"typescript": "^5.1.6",
		"vite": "^4.4.4",
		"vitest": "^0.33.0"
	}
}
