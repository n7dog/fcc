{
  "name": "fcc",
  "version": "3.4.2",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "npx playwright test",
    "test:dev": "npx playwright test tests/dev/",
    "test:prod": "npx playwright test tests/prod/",
    "test:ui": "playwright test --ui",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "fresh": "rm -rf node_modules .svelte-kit playwright-report test-results package-lock.json && npm i && npm run format",
    "fresh:start": "rm -rf node_modules .svelte-kit playwright-report test-results package-lock.json && npm i && npm run format && npm run dev"
  },
  "devDependencies": {
    "@iconify/svelte": "^3.1.3",
    "@playwright/test": "^1.33.0",
    "@sanity/client": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^10.0.2",
    "@skeletonlabs/skeleton": "^1.5.1",
    "@sveltejs/adapter-auto": "^2.0.1",
    "@sveltejs/kit": "^1.16.3",
    "@tailwindcss/forms": "^0.5.3",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.23",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.7",
    "prettier-plugin-svelte": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "semantic-release": "^21.0.1",
    "svelte": "^3.59.1",
    "svelte-check": "^3.3.2",
    "svelte-preprocess": "^5.0.3",
    "svelte-youtube-embed": "^0.1.11",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vitest": "^0.31.0"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.2.7",
    "@js-temporal/polyfill": "^0.4.3",
    "svelte-icons": "^2.1.0"
  }
}
